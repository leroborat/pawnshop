        <!-- TICKET 1: Active Pledged Ticket (Gold Ring) -->
        <record id="pawn_ticket_001" model="pawn.ticket">
            <field name="ticket_no">MAIN-PT-251022-0001</field>
            <field name="customer_id" ref="pawn_customer_juan_dela_cruz"/>
            <field name="branch_id" ref="pawn_branch_main"/>
            <field name="date_created" eval="(DateTime.now() - timedelta(days=15)).strftime('%Y-%m-%d %H:%M:%S')"/>
